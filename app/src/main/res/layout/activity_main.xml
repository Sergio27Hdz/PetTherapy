<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView23"
        android:layout_width="411dp"
        android:layout_height="78dp"
        android:layout_x="0dp"
        android:layout_y="667dp"
        android:background="@color/amarillo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginEnd="285dp"
        android:layout_marginRight="285dp"
        android:layout_x="36dp"
        android:layout_y="680dp"
        android:background="@drawable/home"
        android:onClick="BotonHome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <ImageButton

        android:id="@+id/imageButton12"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginStart="185dp"
        android:layout_x="238dp"
        android:layout_y="682dp"
        android:background="@drawable/chat"
        android:onClick="BotonChat"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/imageButton13"
        app:layout_constraintStart_toStartOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:layout_marginStart="255dp"
        android:layout_x="328dp"
        android:layout_y="680dp"
        android:background="@drawable/perfil"
        android:onClick="BotonPerfil"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toEndOf="@+id/textView23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="39dp"
        android:layout_height="36dp"
        android:layout_x="137dp"
        android:layout_y="682dp"
        android:background="@drawable/buscar"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/imageButton12"
        app:layout_constraintStart_toEndOf="@+id/imageButton11"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <LinearLayout

        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="454dp"
        android:layout_x="0dp"
        android:layout_y="0dp"
        android:background="@drawable/fondo_usuario_inicio"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/imageView2"
        android:layout_width="33dp"
        android:layout_height="27dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:layout_x="348dp"
        android:layout_y="6dp"
        android:background="@mipmap/eua_ico"
        android:onClick="changeLanguageEN"
        android:orientation="vertical"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/imageView"
        android:layout_width="33dp"
        android:layout_height="27dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:layout_x="293dp"
        android:layout_y="6dp"
        android:background="@mipmap/mex_ico"
        android:onClick="changeLanguageES"
        android:orientation="vertical"
        android:padding="5dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_x="11dp"
        android:layout_y="15dp"
        android:onClick="BotonLogOut"
        android:background="@drawable/cerrar"
        android:textColor="@android:color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="343dp"
        android:layout_height="440dp"
        android:layout_marginTop="100dp"
        android:layout_x="73dp"
        android:layout_y="324dp"
        android:fontFamily="@font/roboto_light"
        android:gravity="center"
        android:text="@string/welcome_message"
        android:textColor="#545454"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="225dp"
        android:layout_weight="1"
        android:text="@string/welcome_message_admin_title"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView23"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>