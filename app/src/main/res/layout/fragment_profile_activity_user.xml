<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <AbsoluteLayout
        android:id="@+id/absoluteLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        tools:layout_editor_absoluteX="160dp"
        tools:layout_editor_absoluteY="586dp">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profilePic3"
            android:layout_width="202dp"
            android:layout_height="189dp"
            android:layout_x="75dp"
            android:layout_y="189dp"
            android:visibility="visible"
            tools:src="@tools:sample/avatars" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_foto3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="280dp"
            android:layout_y="228dp"
            android:clickable="true"
            app:backgroundTint="#E1E1E1"
            app:srcCompat="@drawable/ic_baseline_photo_camera_24" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btn_galeria3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="280dp"
            android:layout_y="298dp"
            android:clickable="true"
            app:backgroundTint="#E1E1E1"
            app:srcCompat="@drawable/ic_baseline_image_24" />

        <ImageView
            android:id="@+id/imageView42"
            android:layout_width="486dp"
            android:layout_height="543dp"
            android:layout_x="-57dp"
            android:layout_y="-34dp"
            android:src="@drawable/fondo_usuario"
            android:visibility="visible" />

        <ImageView
            android:id="@+id/imageView43"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="-2dp"
            android:layout_y="-56dp"
            android:src="@drawable/animales_ico"
            android:visibility="visible" />

        <TextView
            android:id="@+id/txtApellido"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="170dp"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="430dp"
            android:layout_x="0dp"
            android:layout_y="424dp"
            android:gravity="center"
            android:text="Apellido"
            android:textSize="30sp" />

        <TextView
            android:id="@+id/txtEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="500dp"
            android:layout_marginLeft="500dp"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="100dp"
            android:layout_marginRight="100dp"
            android:layout_x="0dp"
            android:layout_y="479dp"
            android:gravity="center"
            android:text="Correo"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txtTelefono"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="210dp"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="265dp"
            android:layout_x="3dp"
            android:layout_y="504dp"
            android:gravity="center"
            android:text="Telefono"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txtNombre"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="210dp"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="265dp"
            android:layout_x="0dp"
            android:layout_y="393dp"
            android:gravity="center"
            android:text="Nombre"
            android:textSize="30sp" />

    </AbsoluteLayout>

    <TextView
        android:id="@+id/textView40"
        android:layout_width="411dp"
        android:layout_height="78dp"
        android:layout_x="0dp"
        android:layout_y="667dp"
        android:background="@color/amarillo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/imageButton26"
        android:layout_width="43dp"
        android:layout_height="44dp"
        android:layout_marginStart="255dp"
        android:layout_x="328dp"
        android:layout_y="680dp"
        android:background="@drawable/perfil"
        android:onClick="BotonPerfil"
        app:layout_constraintBottom_toBottomOf="@+id/textView40"
        app:layout_constraintEnd_toEndOf="@+id/textView40"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView40" />

    <ImageButton
        android:id="@+id/imageButton27"
        android:layout_width="39dp"
        android:layout_height="36dp"
        android:layout_x="137dp"
        android:layout_y="682dp"
        android:background="@drawable/buscar"
        android:onClick="BotonBuscar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton28"
        app:layout_constraintStart_toEndOf="@+id/imageButton29"
        app:layout_constraintTop_toTopOf="@+id/textView40" />

    <ImageButton

        android:id="@+id/imageButton28"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginStart="185dp"
        android:layout_x="238dp"
        android:layout_y="682dp"
        android:background="@drawable/chat"
        android:onClick="BotonChat"
        app:layout_constraintBottom_toBottomOf="@+id/textView40"
        app:layout_constraintEnd_toStartOf="@+id/imageButton26"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toTopOf="@+id/textView40" />

    <ImageButton
        android:id="@+id/imageButton29"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginEnd="285dp"
        android:layout_x="36dp"
        android:layout_y="680dp"
        android:background="@drawable/home"
        android:onClick="BotonHome"
        app:layout_constraintBottom_toBottomOf="@+id/textView40"
        app:layout_constraintEnd_toEndOf="@+id/textView40"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView40" />

</androidx.constraintlayout.widget.ConstraintLayout>